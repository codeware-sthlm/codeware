{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "eslint.validate": ["json"],
  "jest.runMode": {
    "coverage": true,
    "deferred": true,
    "runAllTestsOnStartup": false,
    "type": "on-save"
  },
  "[json]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "never"
    }
  }
}
