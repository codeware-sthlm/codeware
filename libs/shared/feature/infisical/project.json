{
  "name": "shared-feature-infisical",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/feature/infisical/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:feature", "domain:apps"],
  "targets": {
    "build": {
      "metadata": {
        "description": "Build is required since this project is used by a package"
      },
      "executor": "@nx/esbuild:esbuild"
    },
    "integration-test": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["default", "^production"],
      "outputs": [
        "{workspaceRoot}/coverage/libs/shared/feature/infisical/integration-tests"
      ],
      "options": {
        "cwd": "{projectRoot}",
        "command": "vitest --config vitest.integration.config.mts"
      },
      "configurations": {
        "ci": {
          "coverage": true
        }
      }
    }
  }
}
